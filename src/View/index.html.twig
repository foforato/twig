{% extends'base.html.twig' %}

{% block title %}
    <title>Quete twig</title>
{% endblock %}

{% block content %}
    <div class="row">
        {% for product in products %}
            <div class="col-sm-4">
                <div class="card">
                    <div class="card-body">
                        <h5 class="card-title">{{ product }}</h5>
                        <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
                    </div>
                </div>
            </div>
        {% endfor %}
    </div>
{% endblock %}